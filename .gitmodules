[submodule "bashdot"]
	path = bashdot
	url = https://github.com/bashdot/bashdot
[submodule "vim-plug"]
	path = default/vim/autoload
	url = https://github.com/junegunn/vim-plug
[submodule "tpm"]
	path = default/tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "bash-sensible"]
	path = default/bash-sensible
	url = https://github.com/mrzool/bash-sensible/
[submodule "default/zgen"]
	path = default/zgen
	url = https://github.com/tarjoilija/zgen.git
[submodule "dotfiles"]
	path = dotfiles
	url = https://github.com/jbernard/dotfiles
